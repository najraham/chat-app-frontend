<template>\n  <!-- <div class=\"page-content page-container\" id=\"page-content\">\n    <div class=\"padding\"> -->\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-9\">\n                <div class=\"card card-bordered\">\n                    <div class=\"card-header\">\n                        <h4 class=\"card-title\"><strong>Chat</strong></h4> <a class=\"btn btn-xs btn-secondary\" href=\"#\" data-abc=\"true\">Let's Chat App</a>\n                    </div>\n                    <div class=\"ps-container ps-theme-default ps-active-y\" id=\"chat-content\" style=\"overflow-y: scroll !important; height:400px !important;\">\n                        <div v-for=\"message in messages\" :key=\"message._id\" class=\"media media-chat media-chat-reverse\">\n                            <div class=\"media-body\">\n                                <p>{{message.content}}</p>\n                                <!-- <p class=\"meta\"><time datetime=\"2018\">23:58</time></p> -->\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"publisher bt-1 border-light\">\n                        <input v-model=\"message.content\" class=\"publisher-input\" type=\"text\" placeholder=\"Write message\">                        \n                        <a class=\"publisher-btn text-info\" href=\"#\" data-abc=\"true\">\n                        <i class=\"fa fa-paper-plane\" @click=\"sendMessage()\"></i></a> \n                        </div>\n                </div>\n            </div>\n        </div>\n    <!-- </div>\n</div> -->\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'Messages',\n  data() {\n      return {\n          messages: [],\n          message: {\n              content:'',\n              sender_id:1,\n              receiver_id:2\n          }\n      }\n  },\n  mounted(){\n    window.setInterval(() => {\n        this.getAllMessages()\n    }, 10000)\n  },\n  methods: {\n    getAllMessages() {\n        axios.get('http://localhost:5000/api/allMessages')\n        .then((res) => {\n            this.messages = res.data\n        })\n    },\n    sendMessage() {\n        axios.post('http://localhost:5000/api/sendMessage', this.message)\n        .then(() => {\n            this.message.content = \"\"\n        })\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"sourceRoot":"src/components"}]}