essage.sender_id == currentUserId\" class=\"chat-message-right pb-4\">\r\n                    <div class=\"flex-shrink-1 bg-info text-white rounded py-2 px-3 mr-3\">\r\n                        {{message.content}}\r\n                    </div>\r\n                </div>\r\n\r\n                <div v-else class=\"chat-message-left pb-4\">\r\n                    <div class=\"flex-shrink-1 bg-light rounded py-2 px-3 ml-3\">\r\n                        {{message.content}}\r\n                    </div>\r\n                </div>\r\n\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <NewMessage/>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport EventBus from '../../eventBus';\r\nimport axios from 'axios';\r\nimport NewMessage from '../Message/NewMessage'\r\n\r\nexport default {\r\n  name: 'Message',\r\n  components: {\r\n    NewMessage\r\n  },\r\n  data() {\r\n    return {\r\n      receiver: {\r\n        fullname: '',\r\n        id: ''\r\n      },\r\n      currentUserId: localStorage.getItem('userid'),\r\n      messages: []\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    EventBus.$on('newUserSelected', (payLoad) => {\r\n      this.receiver.fullname = payLoad.receiver_name\r\n      this.receiver.id = payLoad.id\r\n      window.setInterval(() => {\r\n        this.loadMessages()\r\n      }, 10000)\r\n    });\r\n  },\r\n\r\n  methods: {\r\n    loadMessages() {\r\n        axios.post('http://localhost:5000/api/UserMessage', \r\n        {\r\n            sender_id: this.currentUserId,\r\n            receiver_id: this.receiver.id\r\n        })\r\n        .then((res) => {\r\n            this.messages = res.data\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n</style>\r\n"]}]}